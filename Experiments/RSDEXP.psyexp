        <Param val="import numpy as np&amp;#10;import colorsys&amp;#10;&amp;#10;def circ_diff_deg(a, b, period=360.0):&amp;#10;    diff = (a - b + period/2.0) % period - period/2.0&amp;#10;    return diff&amp;#10;&amp;#10;def hue_to_rgb(hue_deg):&amp;#10;    h = hue_deg / 360.0&amp;#10;    r, g, b = colorsys.hsv_to_rgb(h, 1.0, 1.0)&amp;#10;    return [(r*2-1), (g*2-1), (b*2-1)]&amp;#10;&amp;#10;def wrap_angle(angle, lower=0, upper=180):&amp;#10;    range_width = upper - lower&amp;#10;    return (angle - lower) % range_width + lower&amp;#10;" valType="extendedCode" updates="constant" name="Before Experiment"/>
        <Param val="" valType="extendedCode" updates="constant" name="Before JS Experiment"/>
        <Param val="" valType="extendedCode" updates="constant" name="Begin Experiment"/>
        <Param val="" valType="extendedCode" updates="constant" name="Begin JS Experiment"/>
        <Param val="" valType="extendedCode" updates="constant" name="Begin JS Routine"/>
        <Param val="# --- 预先生成色轮 (Line Stimuli) ---&amp;#10;# 在Builder里，我们不在每一帧创建，而是一次性创建好，之后只调用 draw()&amp;#10;wheel_lines = []&amp;#10;radius = 5.0&amp;#10;line_width = 4.0 # Builder里线条宽度单位可能不同，根据视觉效果调整&amp;#10;n_segments = 360&amp;#10;&amp;#10;for i in range(n_segments):&amp;#10;    angle = (i / n_segments) * 360.0&amp;#10;    rad = np.radians(angle)&amp;#10;    x1 = radius * np.cos(rad)&amp;#10;    y1 = radius * np.sin(rad)&amp;#10;    x2 = (radius + 0.5) * np.cos(rad) # 长度&amp;#10;    y2 = (radius + 0.5) * np.sin(rad)&amp;#10;    &amp;#10;    # 创建Line对象 (注意 win 是 builder 自动创建的 window 对象)&amp;#10;    line = visual.Line(win, start=(x1, y1), end=(x2, y2),&amp;#10;                       lineColor=hue_to_rgb(angle), lineWidth=line_width,&amp;#10;                       units='deg', colorSpace='rgb')&amp;#10;    wheel_lines.append(line)&amp;#10;&amp;#10;# 创建一个用于反馈的小圆点&amp;#10;color_patch = visual.Circle(win, radius=1.5, pos=(0,0), units='deg', fillColor=[0,0,0], lineColor=None)" valType="extendedCode" updates="constant" name="Begin Routine"/>
