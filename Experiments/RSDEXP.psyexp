        <Param val="# 标记 resp_any 已经记录了数据&amp;#10;anyTaskRecorded = True&amp;#10;&amp;#10;try:&amp;#10;    if finalOriAngle is None:&amp;#10;        finalOriAngle = np.nan&amp;#10;except NameError:&amp;#10;    finalOriAngle = np.nan&amp;#10;try:&amp;#10;    if col_resp_hue is None:&amp;#10;        col_resp_hue = np.nan&amp;#10;except NameError:&amp;#10;    col_resp_hue = np.nan&amp;#10;try:&amp;#10;    if rt_reproduction is None:&amp;#10;        rt_reproduction = np.nan&amp;#10;except NameError:&amp;#10;    rt_reproduction = np.nan&amp;#10;&amp;#10;# ===============================&amp;#10;# WRITE DATA&amp;#10;# ===============================&amp;#10;if selectedTask in ['ori', 'orientation']:&amp;#10;    thisExp.addData(&quot;task&quot;, &quot;orientation&quot;)&amp;#10;    thisExp.addData(&quot;ori_angle&quot;, finalOriAngle)&amp;#10;    thisExp.addData(&quot;col_resp_hue&quot;, np.nan)&amp;#10;    print(f&quot;Orientation task completed - Angle: {finalOriAngle}&quot;)&amp;#10;&amp;#10;elif selectedTask == 'color':&amp;#10;    # ====== 调试打印 ======&amp;#10;    print(&quot;=&quot;*50)&amp;#10;    print(&quot;DEBUG before addData:&quot;)&amp;#10;    print(&quot;  col_resp_hue =&quot;, col_resp_hue, &quot;type:&quot;, type(col_resp_hue))&amp;#10;    print(&quot;  rt_reproduction =&quot;, rt_reproduction, &quot;type:&quot;, type(rt_reproduction))&amp;#10;    print(&quot;=&quot;*50)&amp;#10;    &amp;#10;    thisExp.addData(&quot;task&quot;, &quot;color&quot;)&amp;#10;    thisExp.addData(&quot;ori_angle&quot;, np.nan)&amp;#10;    &amp;#10;    try:&amp;#10;        if not np.isnan(col_resp_hue) and not np.isnan(col_stim_hue):&amp;#10;            col_error = circ_diff_deg(col_resp_hue, col_stim_hue, 360)&amp;#10;        else:&amp;#10;            col_error = np.nan&amp;#10;    except:&amp;#10;        col_error = np.nan&amp;#10;    &amp;#10;    thisExp.addData(&quot;col_resp_hue&quot;, col_resp_hue)&amp;#10;    thisExp.addData(&quot;col_error&quot;, col_error)&amp;#10;    thisExp.addData(&quot;rt_reproduction&quot;, rt_reproduction)&amp;#10;    print(f&quot;Color task completed — Hue: {col_resp_hue}, Error: {col_error}&quot;)&amp;#10;&amp;#10;else:&amp;#10;    thisExp.addData(&quot;task&quot;, &quot;NA&quot;)&amp;#10;    thisExp.addData(&quot;ori_angle&quot;, np.nan)&amp;#10;    thisExp.addData(&quot;col_resp_hue&quot;, np.nan)&amp;#10;    thisExp.addData(&quot;col_error&quot;, np.nan)&amp;#10;    thisExp.addData(&quot;rt_reproduction&quot;, np.nan)&amp;#10;    print(&quot;No task selected!&quot;)&amp;#10;&amp;#10;win.mouseVisible = False&amp;#10;print(&quot;=== Routine Ended ===&quot;)" valType="extendedCode" updates="constant" name="End Routine"/>
