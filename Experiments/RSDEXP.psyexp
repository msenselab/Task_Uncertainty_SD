        <Param val="# --- skip routine safely ---&amp;#10;if not goAny:&amp;#10;    continueRoutine = False&amp;#10;is_on_color_wheel = False&amp;#10;mx = 0&amp;#10;my = 0&amp;#10;d = 999&amp;#10;&amp;#10;responseStage = 1&amp;#10;selectedTask = None&amp;#10;finalOriAngle = None&amp;#10;&amp;#10;mouse_any.clickReset()&amp;#10;prevMousePressed = False&amp;#10;win.mouseVisible = False&amp;#10;&amp;#10;&amp;#10;defaultLW = 2&amp;#10;highlightLW = 4&amp;#10;selectedLW = 7&amp;#10;oriCircle_any.lineWidth = defaultLW&amp;#10;&amp;#10;&amp;#10;if 'oriPointer_any' in dir():&amp;#10;    oriPointer_any.setOpacity(1)&amp;#10;if 'colorPatch_any' in dir():&amp;#10;    colorPatch_any.setOpacity(0)&amp;#10;&amp;#10;&amp;#10;radius = 250.0&amp;#10;width_normal = 80.0&amp;#10;width_thick = 90&amp;#10;n_segments = 360&amp;#10;overlap_deg = 1&amp;#10;lineWidth_normal = 8&amp;#10;lineWidth_thick = 14&amp;#10;&amp;#10;print(&quot;Creating color wheels...&quot;)&amp;#10;&amp;#10;&amp;#10;color_wheel_segments = draw_color_wheel(&amp;#10;    win=win, &amp;#10;    radius=radius, &amp;#10;    width=width_normal,&amp;#10;    n_segments=n_segments, &amp;#10;    overlap_deg=overlap_deg,&amp;#10;    lineWidth=lineWidth_normal&amp;#10;)&amp;#10;&amp;#10;print(&quot;Normal wheel created:&quot;, len(color_wheel_segments), &quot;segments&quot;)&amp;#10;&amp;#10;&amp;#10;color_wheel_thick = draw_color_wheel(&amp;#10;    win=win, &amp;#10;    radius=radius, &amp;#10;    width=width_thick,&amp;#10;    n_segments=n_segments, &amp;#10;    overlap_deg=overlap_deg,&amp;#10;    lineWidth=lineWidth_thick&amp;#10;)&amp;#10;&amp;#10;print(&quot;Thick wheel created:&quot;, len(color_wheel_thick), &quot;segments&quot;)&amp;#10;&amp;#10;color_wheel_inner_r = radius&amp;#10;color_wheel_outer_r = radius + width_normal&amp;#10;&amp;#10;win.mouseVisible = False&amp;#10;firstFrame = True&amp;#10;&amp;#10;col_resp_hue = np.nan&amp;#10;col_error = np.nan&amp;#10;rt_reproduction = np.nan&amp;#10;resp_clock = core.Clock()&amp;#10;mouse_path = []&amp;#10;mouse_hue_path = []&amp;#10;t0 = None&amp;#10;current_hue = 0&amp;#10;" valType="extendedCode" updates="constant" name="Begin Routine"/>
