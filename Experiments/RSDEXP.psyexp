        <Param val="&amp;#10;for seg in color_wheel_thick:&amp;#10;   seg.draw()&amp;#10;&amp;#10;if firstFrame:&amp;#10;    win.mouseVisible = False&amp;#10;    prevMousePressed = False&amp;#10;    firstFrame = False&amp;#10;&amp;#10;else:&amp;#10; &amp;#10;    win.mouseVisible = True&amp;#10;    &amp;#10;    mx, my = mouse_color.getPos()&amp;#10;    cx, cy = oriCircle_color.pos&amp;#10;    r_ori = oriCircle_color.size[0] / 2.0&amp;#10;    d = ((mx - cx)**2 + (my - cy)**2)**0.5&amp;#10;    currentMousePressed = mouse_color.getPressed()[0]&amp;#10;    &amp;#10;&amp;#10;    if mx != 0 or my != 0:&amp;#10;        theta = np.degrees(np.arctan2(my - cy, mx - cx))&amp;#10;        if theta &lt; 0:&amp;#10;            theta += 360&amp;#10;        current_hue = theta&amp;#10;    &amp;#10;    if 'colorPatch_color' in dir():&amp;#10;        color_rgb = hue_to_rgb(current_hue)&amp;#10;        colorPatch_color.setFillColor(color_rgb)&amp;#10;        colorPatch_color.setLineColor(color_rgb)&amp;#10;        colorPatch_color.lineWidth = 5&amp;#10;        colorPatch_color.setOpacity(1)&amp;#10;        colorPatch_color.draw()&amp;#10;    &amp;#10;    if t0 is not None:&amp;#10;        t_now = core.getTime() - t0&amp;#10;        mouse_path.append([mx, my, t_now])&amp;#10;        mouse_hue_path.append([current_hue, t_now])&amp;#10;    &amp;#10;    if currentMousePressed and not prevMousePressed:&amp;#10;        col_resp_hue = current_hue&amp;#10;        rt_reproduction = resp_clock.getTime()&amp;#10;        print(&quot;Color hue recorded:&quot;, round(col_resp_hue, 1), &quot;RT:&quot;, round(rt_reproduction, 3))&amp;#10;        continueRoutine = False&amp;#10;    &amp;#10;    if resp_clock.getTime() &gt; 6.0:&amp;#10;        rt_reproduction = 6.0&amp;#10;        print(&quot;Time limit reached (6s)&quot;)&amp;#10;        continueRoutine = False&amp;#10;    &amp;#10;    prevMousePressed = currentMousePressed" valType="extendedCode" updates="constant" name="Each Frame"/>
