        <Param val="&amp;#10;if frameN &lt; 1:&amp;#10;    win.mouseVisible = False&amp;#10;    prevMousePressed = False&amp;#10;&amp;#10;    for seg in color_wheel_segments:&amp;#10;        seg.draw()&amp;#10;&amp;#10;else:&amp;#10;    win.mouseVisible = True&amp;#10;&amp;#10;# win.mouseVisible = True&amp;#10;mx, my = mouse_any.getPos()&amp;#10;cx, cy = oriCircle_any.pos&amp;#10;r_ori = oriCircle_any.size[0] / 2.0&amp;#10;d = ((mx - cx)**2 + (my - cy)**2)**0.5&amp;#10;&amp;#10;currentMousePressed = mouse_any.getPressed()[0]&amp;#10;&amp;#10;&amp;#10;is_on_color_wheel = (color_wheel_inner_r &lt;= d &lt;= color_wheel_outer_r)&amp;#10;&amp;#10;&amp;#10;if frameN % 30 == 0:&amp;#10;    print(&quot;Distance:&quot;, round(d, 1), &quot;| On wheel:&quot;, is_on_color_wheel, &quot;| Stage:&quot;, responseStage, &quot;| Task:&quot;, selectedTask)&amp;#10;&amp;#10;# ============================================================&amp;#10;#  STAGE 1 — SELECT TASK&amp;#10;# ============================================================&amp;#10;if responseStage == 1:&amp;#10;    &amp;#10;   &amp;#10;    if is_on_color_wheel:&amp;#10;        if frameN % 30 == 0:&amp;#10;            print(&quot;&gt;&gt;&gt; DRAWING THICK WHEEL &lt;&lt;&lt;&quot;)&amp;#10;        for seg in color_wheel_thick:&amp;#10;            seg.draw()&amp;#10;    else:&amp;#10;        if frameN % 30 == 0:&amp;#10;            print(&quot;&gt;&gt;&gt; DRAWING NORMAL WHEEL &lt;&lt;&lt;&quot;)&amp;#10;        for seg in color_wheel_segments:&amp;#10;            seg.draw()&amp;#10;    &amp;#10; &amp;#10;    if d &lt;= r_ori:&amp;#10;        oriCircle_any.lineWidth = highlightLW&amp;#10;    else:&amp;#10;        oriCircle_any.lineWidth = defaultLW&amp;#10;&amp;#10;&amp;#10;    if currentMousePressed and not prevMousePressed:&amp;#10;        if d &lt;= r_ori:&amp;#10;            selectedTask = 'ori'&amp;#10;            print(&quot;&gt;&gt;&gt; Selected: ORIENTATION&quot;)&amp;#10;        elif is_on_color_wheel:&amp;#10;            selectedTask = 'color'&amp;#10;            if 'col_stim_hue' in dir():&amp;#10;                current_hue = (col_stim_hue + np.random.uniform(-60, 60)) % 360&amp;#10;            else:&amp;#10;                current_hue = np.random.uniform(0, 360)&amp;#10;            resp_clock.reset()&amp;#10;            t0 = core.getTime()&amp;#10;            mouse_path = []&amp;#10;            mouse_hue_path = []&amp;#10;            print(&quot;&gt;&gt;&gt; Selected: COLOR - Initial hue:&quot;, round(current_hue, 1))&amp;#10;        else:&amp;#10;            selectedTask = None&amp;#10;        &amp;#10;        if selectedTask is not None:&amp;#10;            responseStage = 2&amp;#10;            prevMousePressed = True&amp;#10;&amp;#10;# ============================================================&amp;#10;#  STAGE 2 — RESPONSE&amp;#10;# ============================================================&amp;#10;elif responseStage == 2:&amp;#10;    &amp;#10;   &amp;#10;    if selectedTask == 'color':&amp;#10;        if frameN % 30 == 0:&amp;#10;            print(&quot;&gt;&gt;&gt; STAGE 2 - DRAWING THICK WHEEL &lt;&lt;&lt;&quot;)&amp;#10;        for seg in color_wheel_thick:&amp;#10;            seg.draw()&amp;#10;        oriPointer_any.setOpacity(0)&amp;#10;    else:&amp;#10;        if frameN % 30 == 0:&amp;#10;            print(&quot;&gt;&gt;&gt; STAGE 2 - DRAWING NORMAL WHEEL &lt;&lt;&lt;&quot;)&amp;#10;        for seg in color_wheel_segments:&amp;#10;            seg.draw()&amp;#10;    &amp;#10;    &amp;#10;    if selectedTask == 'ori':&amp;#10;        oriCircle_any.lineWidth = selectedLW&amp;#10;    else:&amp;#10;        oriCircle_any.lineWidth = defaultLW&amp;#10;&amp;#10;    # ORIENTATION TASK&amp;#10;    if selectedTask == 'ori':&amp;#10;        currentAngle = np.degrees(np.arctan2(my - cy, mx - cx))&amp;#10;        &amp;#10;        if 'oriPointer_any' in dir():&amp;#10;            oriPointer_any.setOri(currentAngle)&amp;#10;            oriPointer_any.setOpacity(1)&amp;#10;        &amp;#10;        if currentMousePressed and not prevMousePressed:&amp;#10;            finalOriAngle = currentAngle&amp;#10;            print(&quot;Orientation angle recorded:&quot;, finalOriAngle)&amp;#10;            continueRoutine = False&amp;#10;    &amp;#10;    # COLOR TASK&amp;#10;    elif selectedTask == 'color':&amp;#10;        if mx != 0 or my != 0:&amp;#10;            theta = np.degrees(np.arctan2(my - cy, mx - cx))&amp;#10;            if theta &lt; 0:&amp;#10;                theta += 360&amp;#10;            current_hue = theta&amp;#10;        &amp;#10;        if 'colorPatch_any' in dir():&amp;#10;            color_rgb = hue_to_rgb(current_hue)&amp;#10;            colorPatch_any.setFillColor(color_rgb)&amp;#10;            colorPatch_any.setLineColor(color_rgb)&amp;#10;            colorPatch_any.lineWidth = 5&amp;#10;            colorPatch_any.setOpacity(1)&amp;#10;            colorPatch_any.draw()&amp;#10;        &amp;#10;        if t0 is not None:&amp;#10;            t_now = core.getTime() - t0&amp;#10;            mouse_path.append([mx, my, t_now])&amp;#10;            mouse_hue_path.append([current_hue, t_now])&amp;#10;        &amp;#10;        if currentMousePressed and not prevMousePressed:&amp;#10;            col_resp_hue = current_hue&amp;#10;            rt_reproduction = resp_clock.getTime()&amp;#10;            print(&quot;Color hue recorded:&quot;, round(col_resp_hue, 1), &quot;RT:&quot;, round(rt_reproduction, 3))&amp;#10;            continueRoutine = False&amp;#10;        &amp;#10;        if resp_clock.getTime() &gt; 6.0:&amp;#10;            rt_reproduction = 6.0&amp;#10;            print(&quot;Time limit reached (6s)&quot;)&amp;#10;            continueRoutine = False&amp;#10;&amp;#10;&amp;#10;# ---- 在第一帧 flip 后才显示鼠标 ----&amp;#10;if firstFrame:&amp;#10;    core.wait(0)   # 让 builder 完成第一次 flip&amp;#10;    win.mouseVisible = True&amp;#10;    firstFrame = False&amp;#10;&amp;#10;if responseStage == 2 or not currentMousePressed:&amp;#10;    prevMousePressed = currentMousePressed" valType="extendedCode" updates="constant" name="Each Frame"/>
