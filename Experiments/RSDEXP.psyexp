      <CodeComponent name="code_any" plugin="None">
        <Param val="import numpy as np&amp;#10;import colorsys&amp;#10;from psychopy import visual&amp;#10;def hue_to_rgb(hue_deg):&amp;#10;    &quot;&quot;&quot;&amp;#10;&amp;#10;    &quot;&quot;&quot;&amp;#10;    h = hue_deg / 360.0&amp;#10;    r, g, b = colorsys.hsv_to_rgb(h, 1.0, 1.0)&amp;#10;    return [r*2-1, g*2-1, b*2-1]&amp;#10;&amp;#10;def circ_diff_deg(a, b, period=360.0):&amp;#10;    &quot;&quot;&quot;&amp;#10;    计算两个角度之间的循环差值&amp;#10;    例如：circ_diff_deg(10, 350) = 20 (不是 -340)&amp;#10;    &quot;&quot;&quot;&amp;#10;    diff = (a - b + period/2.0) % period - period/2.0&amp;#10;    return diff&amp;#10;&amp;#10;def draw_color_wheel(win, radius, width, n_segments=180, overlap_deg=2, lineWidth=10):&amp;#10;    &quot;&quot;&quot;&amp;#10;&amp;#10;    &quot;&quot;&quot;&amp;#10;    segments = []&amp;#10;    &amp;#10;    for i in range(n_segments):&amp;#10;        hue = i * (360.0 / n_segments)  &amp;#10;        angle = hue + overlap_deg&amp;#10;        rad = np.radians(angle)&amp;#10;        &amp;#10;     &amp;#10;        x1 = radius * np.cos(rad)&amp;#10;        y1 = radius * np.sin(rad)&amp;#10;        x2 = (radius + width) * np.cos(rad)&amp;#10;        y2 = (radius + width) * np.sin(rad)&amp;#10;        &amp;#10;        &amp;#10;        seg = visual.Line(&amp;#10;            win,&amp;#10;            start=(x1, y1),&amp;#10;            end=(x2, y2),&amp;#10;            lineColor=hue_to_rgb(hue),&amp;#10;            lineWidth=lineWidth,&amp;#10;            units='pix'&amp;#10;        )&amp;#10;        segments.append(seg)&amp;#10;    &amp;#10;    return segments" valType="extendedCode" updates="constant" name="Before Experiment"/>
