      <CodeComponent name="code_2" plugin="None">
        <Param val="" valType="extendedCode" updates="constant" name="Before Experiment"/>
        <Param val="" valType="extendedCode" updates="constant" name="Before JS Experiment"/>
        <Param val="import numpy as np&amp;#10;&amp;#10;rng = np.random.default_rng()&amp;#10;&amp;#10;# build 180 hue wheel (0,2,4,...,358)&amp;#10;hue_wheel = np.linspace(0, 360, 180, endpoint=False)" valType="extendedCode" updates="constant" name="Begin Experiment"/>
        <Param val="" valType="extendedCode" updates="constant" name="Begin JS Experiment"/>
        <Param val="" valType="extendedCode" updates="constant" name="Begin JS Routine"/>
        <Param val="# --------------------------&amp;#10;# 1. Stimulus generation&amp;#10;# --------------------------&amp;#10;&amp;#10;# orientation: continuous 0-180°&amp;#10;ori_stim = rng.uniform(0, 180)&amp;#10;phase_stim = np.random.uniform(0, 2 * np.pi)&amp;#10;&amp;#10;# color: from 180-hue discrete wheel&amp;#10;col_stim_hue = rng.choice(hue_wheel)&amp;#10;&amp;#10;&amp;#10;# --------------------------&amp;#10;# 2. Cue type and actual task&amp;#10;# --------------------------&amp;#10;&amp;#10;# O = orientation cue&amp;#10;# C = color cue&amp;#10;# A = ambiguous cue&amp;#10;types = ['O', 'C', 'A', 'A']   # 25%, 25%, 50%&amp;#10;cueType = rng.choice(types)&amp;#10;&amp;#10;if cueType == 'O':&amp;#10;    actualTask = 'orientation'&amp;#10;elif cueType == 'C':&amp;#10;    actualTask = 'color'&amp;#10;else:&amp;#10;    actualTask = rng.choice(['orientation', 'color'])&amp;#10;    &amp;#10;&amp;#10;&amp;#10;# 1. 定义颜色&amp;#10;target_rgb = np.array(hue_to_rgb(col_stim_hue)) &amp;#10;&amp;#10;# 定义背景灰色&amp;#10;bg_rgb = np.array([0, 0, 0]) &amp;#10;&amp;#10;# 2. 创建正弦波 (保持不变)&amp;#10;# 将 [-1, 1] 映射到 [0, 1]&amp;#10;res = 256&amp;#10;x = np.linspace(0, 2*np.pi, res)&amp;#10;sine_01 = (np.sin(x) + 1) / 2&amp;#10;&amp;#10;# 3. 创建纹理矩阵&amp;#10;# 现在的逻辑是：在 灰色(bg_rgb) 和 目标色(target_rgb) 之间插值&amp;#10;diff = target_rgb - bg_rgb&amp;#10;# 利用广播机制计算：基底色 + 强度 * (色差)&amp;#10;my_texture = bg_rgb + sine_01[:, None, None] * diff&amp;#10;# 注意：my_texture 的形状现在是 (256, 1, 3)，可以直接作为纹理传给 GratingStim&amp;#10;&amp;#10;&amp;#10;&amp;#10;" valType="extendedCode" updates="constant" name="Begin Routine"/>
        <Param val="Py" valType="str" updates="None" name="Code Type"/>
        <Param val="" valType="extendedCode" updates="constant" name="Each Frame"/>
        <Param val="" valType="extendedCode" updates="constant" name="Each JS Frame"/>
        <Param val="" valType="extendedCode" updates="constant" name="End Experiment"/>
        <Param val="" valType="extendedCode" updates="constant" name="End JS Experiment"/>
        <Param val="" valType="extendedCode" updates="constant" name="End JS Routine"/>
        <Param val="# ========== END ROUTINE ==========&amp;#10;&amp;#10;# === STIMULUS INFO ===&amp;#10;thisExp.addData('ori_stim', ori_stim)&amp;#10;thisExp.addData('col_stim_hue', col_stim_hue)&amp;#10;thisExp.addData('cueType', cueType)&amp;#10;thisExp.addData('actualTask', actualTask)&amp;#10;&amp;#10;&amp;#10;&amp;#10;" valType="extendedCode" updates="constant" name="End Routine"/>
